<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>netcoreapp3.1</TargetFramework>
		<AzureFunctionsVersion>v3</AzureFunctionsVersion>
		<PackageId>GeneXus.AzureFunctions.ServiceBusHandler</PackageId>
		<OverridePackable>true</OverridePackable>
	</PropertyGroup>

    <ItemGroup>
        <PackageReference Include="Microsoft.Azure.ServiceBus" Version="5.1.1" />
        <PackageReference Include="Microsoft.Azure.WebJobs.Extensions.ServiceBus" Version="4.2.1" />
        <PackageReference Include="Microsoft.Data.SqlClient" Version="1.0.19269.1" />
        <PackageReference Include="Microsoft.NET.Sdk.Functions" Version="3.0.11" />
        <PackageReference Include="System.IdentityModel.Tokens.Jwt" Version="6.5.1" />
    </ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\..\GxClasses\GxClasses.csproj" />
	</ItemGroup>

	<ItemGroup>
		<Reference Include="GeneXusServerlessAPI">
			<HintPath>..\..\libs\GeneXusServerlessAPI.dll</HintPath>
		</Reference>
	</ItemGroup>
	<ItemGroup>
		<Content Include="$(OutDir)\bin\function.deps.json">
			<Pack>true</Pack>
			<PackagePath>\lib\$(TargetFramework)</PackagePath>
		</Content>
		<Content Include="$(OutDir)\bin\extensions.json">
			<Pack>true</Pack>
			<PackagePath>\lib\$(TargetFramework)</PackagePath>
		</Content>
	</ItemGroup>

</Project>
